{
	"name": "angelcruz/lune",
	"description": "Simple PHP framework inspired by laravel",
	"type": "library",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Lune\\": "src/"
		}
	},
	"scripts": {
		"tests": "phpunit --color=always --testdox tests/",
		"fix": "php-cs-fixer fix --diff"
		
	},
	"autoload-dev": {
		"psr-4": {
			"Lune\\Test\\": "tests"
		}
	},
	"authors": [
		{
			"name": "Angel Cruz",
			"email": "angelcruzsanchez126@gmail.com"
		}
	],
	"require-dev": {
		"phpunit/phpunit": "^10.5",
		"friendsofphp/php-cs-fixer": "^3.56"
	}
}
